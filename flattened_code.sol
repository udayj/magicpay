                }
            }
        }
    }
    function setInterval(uint256 _interval) public {
        interval=_interval;
    }

    function setWaitInterval(uint256 _waitInterval) public {
        waitInterval = _waitInterval;
    }

    function getId() public view returns(uint256) {
        return id;
    }
